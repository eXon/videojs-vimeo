{
  "name": "videojs-vimeo",
  "version": "2.1.0",
  "description": "Allows you to use Vimeo URL as source with Video.js.",
  "main": "dist/vimeo.js",
  "scripts": {
    "start": "npm run build && lite-server -c ./bs-config.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "mkdirp dist && cp src/Vimeo.js dist/Vimeo.js && uglifyjs src/Vimeo.js -o dist/Vimeo.min.js",
    "lint": "jshint ./src/Vimeo.js",
    "validate": "npm ls",
    "version": "npm run build && git add -f dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eXon/videojs-vimeo.git"
  },
  "keywords": [
    "video.js",
    "vimeo",
    "plugin"
  ],
  "author": "Benoit Tremblay <trembl.ben@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eXon/videojs-vimeo/issues"
  },
  "homepage": "https://github.com/eXon/videojs-vimeo#readme",
  "devDependencies": {
    "@vimeo/player": "^2.0.1",
    "jquery": "^2.1.1",
    "jshint": "^2.9.4",
    "lite-server": "^2.2.2",
    "mkdirp": "^0.5.1",
    "uglify-js": "^2.7.5",
    "video.js": "^5.15.1"
  }
}
